<?xml version="1.0" encoding="UTF-8"?>
<Document>
	<Noise>
        <Render>Ground_Shape</Render>
        <Module Type="Fractal" Name="Ground_Shape">
            <Fractal Type="ENUM">HYBRIDMULTI</Fractal>
            <Basis Type="ENUM">SIMPLEX</Basis>
            <Interpolation Type="ENUM">QUINTIC</Interpolation>
            <Octaves Type="Int">3</Octaves>
            <Frequency Type="Double">0.5</Frequency>
        </Module>
        <Module Type="Sphere" Name="TestSphere">
            <Radius Type="Double">2</Radius>
            <CenterX Type="Double">1</CenterX>
            <CenterY Type="Double">1</CenterY>
            <CenterZ Type="Double">1</CenterZ>
        </Module>
        <Module Type="Gradient" Name="Ground_Gradient">
            <X1 Type="Double">0</X1>
            <X2 Type="Double">0</X2>
            <Y1 Type="Double">1</Y1>
            <Y2 Type="Double">0</Y2>
        </Module>
        <Module Type="ScaleOffset" Name="Ground_Scale">
            <Scale Type="Double">0.5</Scale>
            <Offset Type="Double">0</Offset>
            <Source Type="Noise">Ground_Shape</Source>
        </Module>
        <Module Type="TranslateDomain" Name="Ground">
            <Source Type="Noise">Ground_Gradient</Source>
            <YAxis Type="Noise">Ground_Scale</YAxis>
        </Module>
	</Noise>
</Document>